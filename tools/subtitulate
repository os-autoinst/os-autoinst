use strict;
use subtitle;

# usage: $0 testresults/autoinst-log.txt > video.vtt
# this uses a log (created with DEVEL_SUBTITLES=1) as source
# and produces subtitles in the WebVTT standard

subtitle::init_subtitle("/dev/stdout");

while(<>) {
	subtitle::add_subtitle_line($_);
}

subtitle::finish_subtitle;
